{
	"expo": {
		"name": "Slide-Jobscapw",
		"slug": "Slide-Jobscapw",
		"version": "1.0.0",
		"orientation": "portrait",
		"scheme": "jobscape",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"ios": {
			"supportsTablet": true,
			"infoPlist": {
				"NSPhotoLibraryUsageDescription": "This app needs photo library access to allow you to select your company logo or resume."
			}
		},
		"android": {
			"adaptiveIcon": {
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true
		},
		"web": {
			"bundler": "metro",
			"output": "static"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#ffffff"
				}
			],
			[
				"expo-image-picker",
				{
					"photosPermission": "This app needs photo library access to allow you to select your company logo or resume."
				}
			],
			"expo-font"
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
